{
  "name": "symbiotic-analysis-environment",
  "version": "0.1.0",
  "description": "A local-first, AI-powered data analysis environment",
  "main": "src/main.tsx",
  "private": true,
  "author": "Symbiotic Analysis Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/symbiotic-analysis/environment.git"
  },
  "keywords": [
    "tauri",
    "data-analysis",
    "ai",
    "local-first",
    "desktop-app"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "build:installers": "node build-installers.js",
    "test:config": "node test-installer-config.js",
    "test:installers": "node build-installers.js --test",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:a11y": "node scripts/run-accessibility-tests.js",
    "test:a11y:watch": "vitest --config vitest.config.accessibility.ts",
    "test:a11y:coverage": "vitest run --config vitest.config.accessibility.ts --coverage",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css}",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@codemirror/autocomplete": "^6.18.6",
    "@codemirror/basic-setup": "^0.20.0",
    "@codemirror/commands": "^6.3.3",
    "@codemirror/fold": "^0.19.4",
    "@codemirror/lang-sql": "^6.6.1",
    "@codemirror/language": "^6.10.1",
    "@codemirror/lint": "^6.8.5",
    "@codemirror/search": "^6.5.6",
    "@codemirror/state": "^6.4.1",
    "@codemirror/theme-one-dark": "^6.1.2",
    "@codemirror/view": "^6.26.3",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.12",
    "@tauri-apps/api": "^2.7.0",
    "@tauri-apps/cli": "^2.7.1",
    "@uiw/react-codemirror": "^4.21.25",
    "react": "^18.2.0",
    "react-arborist": "^3.4.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@axe-core/react": "^4.10.2",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "jest-axe": "^10.0.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
